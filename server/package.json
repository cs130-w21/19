        "ws": "^7.4.3",
        "universal-cookie": "^4.0.4"
    },
    "devDependencies": {
        "apidoc": "^0.26.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "cross-env": "^7.0.3",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.7",
        "sinon": "^9.2.4",
        "sinon-chai": "^3.5.0"
    },
    "proxy": "http://localhost:8080"
}
