{
    "name": "cs130-proj-server",
    "version": "1.0.0",
    "type": "module",
    "description": "Our CS 130 repo",
    "main": "server.js",
    "scripts": {
        "start": "nodemon --ignore docs --ignore node_modules --exec npm run dev-server",
        "dev-server": "npm run doc && cross-env NODE_ENV='stonks-dev' node server.js",
        "unit-test": "mocha 'test/unit/*.test.js' --exit",
        "integration-test": "cross-env mocha --file test/integration/setupTest.js 'test/integration/*.test.js' --timeout 6000 --exit",
        "doc": "apidoc -i src/ -o docs/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cs130-w21/19.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/cs130-w21/19/issues"
    },
    "homepage": "https://github.com/cs130-w21/19#readme",
    "dependencies": {
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "csvtojson": "^2.0.10",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "luxon": "^1.25.0",
        "minisearch": "^3.0.2",
        "node-cron": "^2.0.3",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "pg": "^8.5.1",
        "pg-format": "^1.0.4",
